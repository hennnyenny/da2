using System;

public interface ISoDaoNguoc
{
    static abstract global::System.Int32 DaoNguocSo(global::System.Int32 so);
    static abstract void Main(global::System.String[] args);
}

public class SoDaoNguoc : ISoDaoNguoc
{
    public static int DaoNguocSo(int so)
    {
        int soDaoNguoc = 0;

        while (so != 0)
        {
            int chuSoCuoi = so % 10;
            soDaoNguoc = (soDaoNguoc * 10) + chuSoCuoi;
            so = so / 10;
        }

        return soDaoNguoc;
    }

    public static void Main(string[] args)
    {
        int soBanDau = 12345;
        int soDaoNguoc = DaoNguocSo(soBanDau);
        Console.WriteLine($"Số ban đầu: {soBanDau}");
        Console.WriteLine($"Số đảo ngược: {soDaoNguoc}");
        // Kết quả: 54321
    }
}